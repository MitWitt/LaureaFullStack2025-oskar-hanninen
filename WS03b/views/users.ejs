<%- include("partials/_header") %>
<h1>Käyttäjälista</h1>
<table border="1">
    <tr>
        <th>Nimi</th>
        <th>Email</th>
    </tr>
    <% users.forEach(user => { %>
        <tr>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
        </tr>
    <% }); %>
</table>
<%- include("partials/_footer") %>
